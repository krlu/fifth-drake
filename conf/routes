# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
# GET     /                           controllers.HomeController.index
# GET     /form                       controllers.HomeController.JarTester

# An example controller showing how to write asynchronous code

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file)

POST    /assets/*file               controllers.Assets.versioned(path="/public", file)

GET		/             				controllers.GameDataController.loadHomePage
GET     /lol/games/id/:id/:url      controllers.GameDataController.getGameData(id: String, url: String)
GET     /lol/games/                 controllers.GameDataController.allGames
